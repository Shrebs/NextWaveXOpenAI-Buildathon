    <script>
        // Navigation functionality
        function showSection(sectionId) {
            // Hide all sections
            const sections = document.querySelectorAll('.section');
            sections.forEach(section => {
                section.classList.add('section-hidden');
            });
            
            // Show selected section
            document.getElementById(sectionId).classList.remove('section-hidden');
            
            // Update navigation buttons
            const navButtons = document.querySelectorAll('.nav-btn');
            navButtons.forEach(btn => {
                btn.classList.remove('text-purple-600');
                btn.classList.add('text-gray-700');
            });
            
            // Highlight active button
            event.target.classList.remove('text-gray-700');
            event.target.classList.add('text-purple-600');
            
            // Scroll to top
            window.scrollTo(0, 0);
        }

        // AI Recommendations functionality
        function updateRecommendations(category) {
            const recommendationsDiv = document.getElementById('ai-recommendations');
            
            const recommendations = {
                food: [
                    { name: "Priya's Kitchen", type: "North Indian Tiffin", distance: "0.5 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "95%" },
                    { name: "Sita's South Specials", type: "South Indian Meals", distance: "1.2 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÜ", match: "88%" },
                    { name: "Healthy Bites", type: "Diet-friendly Tiffin", distance: "0.8 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "92%" }
                ],
                beauty: [
                    { name: "Anjali Beauty", type: "Bridal Makeup", distance: "0.3 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "97%" },
                    { name: "Glow Studio", type: "Skincare Specialist", distance: "1.5 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÜ", match: "85%" },
                    { name: "Hair & Beyond", type: "Hair Styling", distance: "0.7 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "90%" }
                ],
                tailoring: [
                    { name: "Meera's Tailoring", type: "Custom Ethnic Wear", distance: "0.4 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "93%" },
                    { name: "Stitch Perfect", type: "Alterations Expert", distance: "1.1 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÜ", match: "87%" },
                    { name: "Designer Touch", type: "Bridal Wear", distance: "0.9 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "91%" }
                ]
            };
            
            const categoryRecs = recommendations[category] || recommendations.food;
            
            recommendationsDiv.innerHTML = categoryRecs.map(rec => `
                <div class="bg-white rounded-lg p-3 border border-gray-200">
                    <div class="flex items-center justify-between">
                        <div>
                            <h6 class="font-medium">${rec.name}</h6>
                            <p class="text-sm text-gray-600">${rec.type} ‚Ä¢ ${rec.distance} away</p>
                        </div>
                        <div class="text-right">
                            <div class="star-rating text-sm">${rec.rating}</div>
                            <p class="text-sm text-purple-600 font-medium">${rec.match} match</p>
                        </div>
                    </div>
                </div>
            `).join('');
            
            // Update button styles
            document.querySelectorAll('.recommendation-btn').forEach(btn => {
                btn.classList.remove('bg-purple-200', 'text-purple-900');
                btn.classList.add('hover:bg-gray-200');
            });
            
            event.target.classList.add('bg-purple-200', 'text-purple-900');
            event.target.classList.remove('hover:bg-gray-200');
        }

        // Initialize with home section
        document.addEventListener('DOMContentLoaded', function() {
            showSection('home');
        });
    </script>
