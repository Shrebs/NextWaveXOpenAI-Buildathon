                    reviews: 178,
                    keywords: ["photography", "photographer", "photo", "picture", "event", "wedding", "portrait", "camera", "shoot", "pre-wedding"]
                },
                home: {
                    name: "Sunita's Home Services",
                    category: "Home Services",
                    icon: "üßπ",
                    description: "Reliable home cleaning, organizing, and pet care services",
                    rating: "‚≠ê‚≠ê‚≠ê‚≠ê‚òÜ (4.6/5)",
                    distance: "0.7 km away",
                    experience: "4 years experience",
                    specialties: ["Home Cleaning", "Organization", "Pet Sitting", "Laundry Services"],
                    priceRange: "‚Çπ500-2,000 per visit",
                    availability: "All days, 8 AM - 6 PM",
                    contact: "+91 98765 43216",
                    reviews: 134,
                    keywords: ["cleaning", "home", "maid", "housekeeping", "pet", "dog", "cat", "organize", "laundry", "wash"]
                }
            };

            // Find best match based on keywords
            let bestMatch = services.food; // default
            let maxScore = 0;

            for (let key in services) {
                let score = 0;
                services[key].keywords.forEach(keyword => {
                    if (query.includes(keyword)) {
                        score += 10;
                    }
                });
                
                if (score > maxScore) {
                    maxScore = score;
                    bestMatch = services[key];
                }
            }

            return bestMatch;
        }

        function displaySearchResults(service) {
            // Create profile view section if it doesn't exist
            let profileSection = document.getElementById('service-profile');
            if (!profileSection) {
                profileSection = document.createElement('section');
                profileSection.id = 'service-profile';
                profileSection.className = 'section section-hidden';
                document.querySelector('footer').before(profileSection);
            }

            // Populate profile section
            profileSection.innerHTML = `
                <div class="container">
                    <button class="btn btn-outline" onclick="showSection('services')" style="margin-bottom: 2rem;">
                        ‚Üê Back to Search
                    </button>
                    
                    <div style="background: var(--card-bg); border-radius: 20px; padding: 3rem; box-shadow: var(--shadow);">
                        <div style="text-align: center; margin-bottom: 2rem;">
                            <div style="font-size: 5rem; margin-bottom: 1rem;">${service.icon}</div>
                            <h1 style="color: var(--primary); font-size: 2.5rem; margin-bottom: 0.5rem;">${service.name}</h1>
                            <p style="color: var(--subtle); font-size: 1.2rem; margin-bottom: 0.5rem;">${service.category}</p>
                            <div style="font-size: 1.3rem; margin-bottom: 0.5rem;">${service.rating}</div>
                            <p style="color: var(--subtle);">${service.reviews} reviews ‚Ä¢ ${service.distance} ‚Ä¢ ${service.experience}</p>
                        </div>

                        <div style="border-top: 2px solid var(--input-border); padding-top: 2rem; margin-top: 2rem;">
                            <h3 style="color: var(--primary); font-size: 1.5rem; margin-bottom: 1rem;">About</h3>
                            <p style="color: var(--text); font-size: 1.1rem; line-height: 1.8; margin-bottom: 2rem;">${service.description}</p>

                            <div class="grid grid-2" style="gap: 2rem; margin-bottom: 2rem;">
                                <div>
                                    <h4 style="color: var(--primary); margin-bottom: 0.8rem; font-size: 1.2rem;">Specialties</h4>
                                    <div style="display: flex; flex-wrap: wrap; gap: 0.5rem;">
                                        ${service.specialties.map(s => `
                                            <span style="background: var(--accent); color: var(--primary); padding: 0.5rem 1rem; border-radius: 20px; font-size: 0.9rem; font-weight: 600;">${s}</span>
                                        `).join('')}
                                    </div>
                                </div>
                                <div>
                                    <h4 style="color: var(--primary); margin-bottom: 0.8rem; font-size: 1.2rem;">Details</h4>
                                    <p style="color: var(--text); margin-bottom: 0.5rem;"><strong>Price Range:</strong> ${service.priceRange}</p>
                                    <p style="color: var(--text); margin-bottom: 0.5rem;"><strong>Availability:</strong> ${service.availability}</p>
                                    <p style="color: var(--text);"><strong>Contact:</strong> ${service.contact}</p>
                                </div>
                            </div>

                            <div style="display: flex; gap: 1rem; justify-content: center; margin-top: 2rem;">
                                <button class="btn" style="font-size: 1.1rem; padding: 1rem 2rem;" onclick="contactService('${service.name}')">
                                    üìû Contact Now
                                </button>
                                <button class="btn btn-outline" style="font-size: 1.1rem; padding: 1rem 2rem;" onclick="bookService('${service.name}')">
                                    üìÖ Book Appointment
                                </button>
                            </div>
                        </div>
                    </div>

                    <div style="margin-top: 3rem;">
                        <h2 style="color: var(--primary); margin-bottom: 1.5rem; font-size: 2rem;">Customer Reviews</h2>
                        <div class="grid grid-2">
                            <div class="card">
                                <div style="display: flex; justify-content: space-between; margin-bottom: 0.5rem;">
                                    <strong style="color: var(--primary);">Neha Sharma</strong>
                                    <span style="color: #d4a574;">‚≠ê‚≠ê‚≠ê‚≠ê‚≠ê</span>
