                    { name: "Anjali Beauty", type: "Bridal Makeup", distance: "0.3 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "97%" },
                    { name: "Glow Studio", type: "Skincare Specialist", distance: "1.5 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÜ", match: "85%" },
                    { name: "Hair & Beyond", type: "Hair Styling", distance: "0.7 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "90%" }
                ],
                tailoring: [
                    { name: "Meera's Tailoring", type: "Custom Ethnic Wear", distance: "0.4 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "93%" },
                    { name: "Stitch Perfect", type: "Alterations Expert", distance: "1.1 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÜ", match: "87%" },
                    { name: "Designer Touch", type: "Bridal Wear", distance: "0.9 km", rating: "‚òÖ‚òÖ‚òÖ‚òÖ‚òÖ", match: "91%" }
                ]
            };

            const categoryRecs = recommendations[category] || recommendations.food;
            const container = document.getElementById('recommendationsList');
            
            container.innerHTML = categoryRecs.map(rec => `
                <div class="recommendation-item">
                    <div class="rec-header">
                        <div class="rec-name">${rec.name}</div>
                        <div class="rec-match">${rec.match} match</div>
                    </div>
                    <div class="rec-details">${rec.type} ‚Ä¢ ${rec.distance} away</div>
                    <div class="rec-rating">${rec.rating}</div>
                </div>
            `).join('');

            // Update button styles
            document.querySelectorAll('.filter-btn').forEach(btn => {
                btn.classList.remove('active');
            });
            event.target.classList.add('active');
        }

        // Initialize recommendations
        updateRecommendations('food');

        // AI Menu Functions
        function openChatbot() {
            const menu = document.getElementById('aiMenu');
            menu.classList.toggle('show');
        }

        // Close AI menu when clicking outside
        document.addEventListener('click', function(event) {
            const aiMenu = document.getElementById('aiMenu');
            const aiButton = event.target.closest('.profile-icon[onclick="openChatbot()"]');
            
            if (!aiButton && !aiMenu.contains(event.target)) {
                aiMenu.classList.remove('show');
            }
        });

        function openChatbotAssistant() {
            document.getElementById('aiMenu').classList.remove('show');
            document.getElementById('chatbotContainer').classList.add('show');
            document.getElementById('chatbotInput').focus();
        }

        function closeChatbot() {
            document.getElementById('chatbotContainer').classList.remove('show');
        }

        function openSmartSearch() {
            document.getElementById('aiMenu').classList.remove('show');
            showSection('ai-features');
            setTimeout(() => {
                document.getElementById('aiSearch').focus();
            }, 300);
        }

        function handleChatEnter(event) {
            if (event.key === 'Enter') {
                sendMessage();
            }
        }

        async function sendMessage() {
            const input = document.getElementById('chatbotInput');
            const message = input.value.trim();
            
            if (!message) return;

            // Add user message
            addChatMessage(message, 'user');
            input.value = '';

            // Disable send button
            const sendBtn = document.getElementById('chatbotSendBtn');
            sendBtn.disabled = true;

            // Show typing indicator
            showTypingIndicator();

            // Simulate AI response
            setTimeout(() => {
                hideTypingIndicator();
                const response = getAIResponse(message.toLowerCase());
                addChatMessage(response, 'bot');
                sendBtn.disabled = false;
            }, 1000 + Math.random() * 1000);
        }

        function addChatMessage(text, sender) {
            const messagesContainer = document.getElementById('chatbotMessages');
            const messageDiv = document.createElement('div');
            messageDiv.className = `chat-message ${sender}`;
            
            const avatar = document.createElement('div');
            avatar.className = `message-avatar ${sender}`;
            avatar.textContent = sender === 'bot' ? 'ü§ñ' : 'ÔøΩÔøΩ';
            
            const bubble = document.createElement('div');
            bubble.className = 'message-bubble';
            bubble.textContent = text;
            
            messageDiv.appendChild(avatar);
            messageDiv.appendChild(bubble);
            messagesContainer.appendChild(messageDiv);
            
            // Scroll to bottom
            messagesContainer.scrollTop = messagesContainer.scrollHeight;
        }

        function showTypingIndicator() {
            const messagesContainer = document.getElementById('chatbotMessages');
            const typingDiv = document.createElement('div');
            typingDiv.className = 'chat-message bot';
            typingDiv.id = 'typingIndicator';
            
            typingDiv.innerHTML = `
                <div class="message-avatar bot">ü§ñ</div>
                <div class="message-bubble">
                    <div class="typing-indicator">
                        <div class="typing-dot"></div>
                        <div class="typing-dot"></div>
                        <div class="typing-dot"></div>
                    </div>
                </div>
            `;
            
            messagesContainer.appendChild(typingDiv);
            messagesContainer.scrollTop = messagesContainer.scrollHeight;
        }

        function hideTypingIndicator() {
            const indicator = document.getElementById('typingIndicator');
            if (indicator) {
                indicator.remove();
            }
        }

        function getAIResponse(message) {
            // Simple keyword-based responses
            if (message.includes('hello') || message.includes('hi') || message.includes('hey')) {
                return "Hello! Welcome to Homepreneur Hub. How can I assist you today?";
            }
            
            if (message.includes('food') || message.includes('tiffin') || message.includes('catering')) {
                return "Great! We have amazing food service providers. You can find homemade tiffin services, catering for events, and baked goods. Would you like me to show you our top-rated food service providers in your area?";
            }
            
            if (message.includes('beauty') || message.includes('makeup') || message.includes('salon')) {
                return "We have talented beauty professionals including makeup artists, mehendi artists, and skincare specialists. Many offer at-home services! Would you like to explore our beauty services section?";
            }
            
            if (message.includes('register') || message.includes('sign up') || message.includes('join')) {
                return "Registration is easy! Just click the 'Sign Up' button at the top. You can register as a Customer to book services or as a Service Owner to offer your services. It only takes a minute!";
            }
            
            if (message.includes('price') || message.includes('cost') || message.includes('fee')) {
                return "Prices vary by service provider and location. Each provider sets their own rates. You can view pricing details on their profiles, and many offer instant quotes through our AI price calculator. Would you like to check out specific services?";
            }
            
            if (message.includes('how') && (message.includes('work') || message.includes('use'))) {
                return "It's simple! Browse services ‚Üí Select a provider ‚Üí Contact them directly or book online. Service owners can create profiles, showcase their work, and connect with customers. Want me to guide you through a specific feature?";
            }
            
            if (message.includes('location') || message.includes('near me') || message.includes('area')) {
                return "We serve providers across 150+ cities in India! Our AI-powered search shows you services available in your neighborhood. You can filter by distance, ratings, and availability. Where are you located?";
            }
            
            if (message.includes('safe') || message.includes('trust') || message.includes('verified')) {
                return "Your safety is our priority! All service providers are verified, and we have a rating and review system. Plus, our community guidelines ensure a safe, supportive environment for all women entrepreneurs and customers.";
            }

            if (message.includes('thank') || message.includes('thanks')) {
                return "You're welcome! I'm here anytime you need help. Feel free to ask anything about our services or platform. üòä";
            }

            // Default response
            return "I'm here to help! I can assist you with finding services, learning about our platform, registration, pricing, and more. Could you tell me more specifically what you're looking for?";
        }

        function performAISearch() {
            const searchQuery = document.getElementById('aiSearch').value.trim().toLowerCase();
            if (!searchQuery) {
                showToast('Please enter a search query', 'error');
                return;
            }
            
            showToast(`Searching for: "${searchQuery}"...`, 'success');
            
            // Analyze query and redirect to matching service profile
            setTimeout(() => {
                let matchedService = findBestMatch(searchQuery);
                displaySearchResults(matchedService);
            }, 1000);
        }

